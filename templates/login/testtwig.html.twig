    $('#time').click(function(){ 
        var min = moment($("#idtemps").val());
                  var time = new Date(min);    
                  month = '' + (time.getMonth()+1),
        day = '' + time.getDate(),
        year = time.getFullYear();
        if (month.length < 2) month = '0' + month;
    if (day.length < 2) day = '0' + day;
    var t= [day, month,year].join('/');
    
                  $('#time-holder').val(t);  
        });
        });
  
